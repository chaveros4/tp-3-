#include <iostream>
using namespace std;

int main() {
    int suma = 0, contador = 0;

    for (int i = 1; i <= 300; i++) {
        if (i % 2 != 0) {
            contador++;
            suma += i;
        }
    }

    cout << "Cantidad de impares: " << contador << endl;
    cout << "Sumatoria de impares: " << suma << endl;

    return 0;
}
